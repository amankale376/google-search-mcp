# API Keys
GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id_here
APOLLO_API_KEY=your_apollo_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_GEMINI_API_KEY=your_google_gemini_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Ollama Configuration (for local AI models)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# Database Configuration
DATABASE_PATH=./data/search.db

# Search Configuration
DEFAULT_SEARCH_DELAY_MS=120000
MAX_SEARCH_RESULTS=100
MAX_LOCATIONS_PER_SEARCH=50
SEARCH_TIMEOUT_MS=30000

# Rate Limiting
GOOGLE_SEARCH_RATE_LIMIT=100
APOLLO_RATE_LIMIT=1000
AI_PROVIDER_RATE_LIMIT=60

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/search-server.log

# Server Configuration
MCP_SERVER_NAME=search-mcp-server
MCP_SERVER_VERSION=1.0.0